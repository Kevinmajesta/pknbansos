<TfrxReport Version="4.10.1" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="40505,5340858218" ReportOptions.Description.Text="" ReportOptions.LastChange="41726,4244768056" ScriptLanguage="PascalScript" ScriptText.Text="var nama_pemohon,pemohon : string;                                             &#13;&#10;function getDay(a: TDateTime): String;&#13;&#10;var s : string;                                   &#13;&#10;begin&#13;&#10;  s := '';&#13;&#10;  case a of&#13;&#10;    1 : s := 'Minggu';  &#13;&#10;    2 : s := 'Senin';  &#13;&#10;    3 : s := 'Selasa';                                                                                           &#13;&#10;    4 : s := 'Rabu';  &#13;&#10;    5 : s := 'Kamis';                                                                                           &#13;&#10;    6 : s := 'Jumat';  &#13;&#10;    7 : s := 'Sabtu';&#13;&#10;  end;&#13;&#10;  result := s;                       &#13;&#10;end;          &#13;&#10;&#13;&#10;function getMonth(a: TDateTime): String;&#13;&#10;var s : string;    &#13;&#10;begin&#13;&#10;  s := '';                           &#13;&#10;  case a of&#13;&#10;    1 : s := 'Januari';  &#13;&#10;    2 : s := 'Februari';  &#13;&#10;    3 : s := 'Maret';                                                                                           &#13;&#10;    4 : s := 'April';  &#13;&#10;    5 : s := 'Mei';                                                                                           &#13;&#10;    6 : s := 'Juni';  &#13;&#10;    7 : s := 'Juli';  &#13;&#10;    8 : s := 'Agustus';  &#13;&#10;    9 : s := 'September';  &#13;&#10;    10 : s := 'Oktober';  &#13;&#10;    11 : s := 'November';  &#13;&#10;    12 : s := 'Desember';&#13;&#10;  end;&#13;&#10;  result := s;                                &#13;&#10;end;&#13;&#10;&#13;&#10;function getNama(a: String): String;&#13;&#10;var s : string;                                 &#13;&#10;begin&#13;&#10;  s := '';                                  &#13;&#10;  case a of                                      &#13;&#10;    'Pemerintah' : s := NameCase(&#60;qryNPHD.&#34;NAMA_PEMOHON&#34;&#62;);                                  &#13;&#10;    'Pemerintah Daerah Lain' : s := NameCase(&#60;qryNPHD.&#34;NAMA_PEMOHON&#34;&#62;);&#13;&#10;    'Perusahaan Daerah' : s := NameCase(&#60;qryNPHD.&#34;NAMA_PIMPINAN&#34;&#62;);                                  &#13;&#10;    'Masyarakat' : s := NameCase(&#60;qryNPHD.&#34;NAMA_PIMPINAN&#34;&#62;);                                  &#13;&#10;    'Organisasi Kemasyarakatan' : s := NameCase(&#60;qryNPHD.&#34;NAMA_PIMPINAN&#34;&#62;);&#13;&#10;  end;&#13;&#10;                          &#13;&#10;  result := s;                              &#13;&#10;end;&#13;&#10;              &#13;&#10;function tanggal_teks(tanggal: TDate):string;&#13;&#10;var &#13;&#10;  hari, thn, bln, tgl : string;&#13;&#10;begin&#13;&#10;  hari := FormatDateTime('dddd', tanggal);&#13;&#10;  tgl := lowercase(terbilangId(DayOf(tanggal)));&#13;&#10;  bln := FormatDateTime('mmmm', tanggal);&#13;&#10;  thn := lowercase(terbilangId(YearOf(tanggal)));                                                               &#13;&#10;  Result := hari + ' tanggal ' + tgl + ' bulan ' + bln + ' tahun ' +thn;                                                                                                                                                                      &#13;&#10;end;          &#13;&#10;&#13;&#10;function getNamaPemohon(a: String): String;&#13;&#10;var s : string;                                 &#13;&#10;begin&#13;&#10;  s := '';                                  &#13;&#10;  case a of                                      &#13;&#10;    'Individu/Keluarga' : s := NameCase(&#60;QryBAST.&#34;NAMA_PEMOHON&#34;&#62;);                                  &#13;&#10;    'Masyarakat' : s := NameCase(&#60;QryBAST.&#34;NAMA_PEMOHON&#34;&#62;);                                  &#13;&#10;    'Lembaga Non Pemerintah' : s := NameCase(&#60;QryBAST.&#34;NAMA_PIMPINAN&#34;&#62;);            &#13;&#10;  end;                            &#13;&#10;  result := s;                              &#13;&#10;end;&#13;&#10;&#13;&#10;function getNamaPemohonBrg(a: String): String;&#13;&#10;var s : string;                                 &#13;&#10;begin&#13;&#10;  s := '';                                  &#13;&#10;  case a of                                      &#13;&#10;    'Pemerintah' : s := NameCase(&#60;QryBAST.&#34;NAMA_PEMOHON&#34;&#62;);&#13;&#10;    'Pemerintah Daerah Lain' : s := NameCase(&#60;QryBAST.&#34;NAMA_PEMOHON&#34;&#62;);&#13;&#10;    'Perusahaan Daerah' : s := NameCase(&#60;QryBAST.&#34;NAMA_PIMPINAN&#34;&#62;);                                  &#13;&#10;    'Masyarakat' : s := NameCase(&#60;QryBAST.&#34;NAMA_PIMPINAN&#34;&#62;);                                  &#13;&#10;    'Organisasi Kemasyarakatan' : s := NameCase(&#60;QryBAST.&#34;NAMA_PIMPINAN&#34;&#62;);&#13;&#10;    'Lembaga Non Pemerintahan' : s := NameCase(&#60;QryBAST.&#34;NAMA_PIMPINAN&#34;&#62;);&#13;&#10;    'Individu/Keluarga' : s := NameCase(&#60;QryBAST.&#34;NAMA_PEMOHON&#34;&#62;);        &#13;&#10;  end;                            &#13;&#10;  result := s;                              &#13;&#10;end;&#13;&#10;    &#13;&#10;function getNIK(a: String): String;&#13;&#10;begin&#13;&#10;  if (a = '') then result := '-'&#13;&#10;  else result := NameCase(&#60;QryBAST.&#34;NIK&#34;&#62;);;                                                                  &#13;&#10;end;  &#13;&#10;  &#13;&#10;function getJabatan(a: String): String;&#13;&#10;begin&#13;&#10;  if (a = '') then result := '-'&#13;&#10;  else result := 'Ketua';                                                                  &#13;&#10;end;  &#13;&#10;  &#13;&#10;begin  &#13;&#10;       frxDBPemda.Open;&#13;&#10;       if frxDBPemda.FieldByName('LOGO').IsNull then&#13;&#10;       begin                   &#13;&#10;       Picture1.Visible := false;&#13;&#10;       Picture2.Visible := false;&#13;&#10;       Picture3.Visible := false;           &#13;&#10;       end;  &#13;&#10;            &#13;&#10;        QryBAST.ParamByName('id').Value            := &#60;id&#62;;&#13;&#10;        QryBAST.ParamByName('tahun').Value         := &#60;tahun&#62;;            &#13;&#10;        QrySKPD.ParamByName('id_skpd').Value         := &#60;id_skpd&#62;;&#13;&#10;        qryTerbilang.ParamByName('nominal').Value := &#60;QryBAST.&#34;NOMINAL&#34;&#62;;  &#13;&#10;&#13;&#10;        QryTipe.Open;&#13;&#10;&#13;&#10;       &#13;&#10;       if(getNamaPemohonBrg(&#60;QryBAST.&#34;KATEGORI&#34;&#62;) &#60;&#62; '') then&#13;&#10;         pemohon := getNamaPemohonBrg(&#60;QryBAST.&#34;KATEGORI&#34;&#62;)&#13;&#10;       else&#13;&#10;         pemohon := '.............................';                                &#13;&#10;                        &#13;&#10;      if(&#60;jenis_bantuan&#62;='Hibah') then&#13;&#10;      begin                  &#13;&#10;          Hibah_BAST.Visible := True;&#13;&#10;          Hibah_PaktaIntegritas.Visible := True;&#13;&#10;          Hibah_TandaTerima.Visible := True;              &#13;&#10;&#13;&#10;          Bansos_BAST.Visible := False;&#13;&#10;          Bansos_SuratPernyataan.Visible := False;&#13;&#10;          Bansos_PaktaIntegritas.Visible := False;&#13;&#10;          Bansos_TandaTerima.Visible := False;&#13;&#10;          Bansos_SuratPernyataanKonflik.Visible := False;&#13;&#10;          Bansos_SuratPernyataanTanggungjawab.Visible := False;              &#13;&#10;      end          &#13;&#10;      else&#13;&#10;      begin                &#13;&#10;          Bansos_BAST.Visible := True;&#13;&#10;          Bansos_SuratPernyataan.Visible := True;&#13;&#10;          Bansos_PaktaIntegritas.Visible := True;&#13;&#10;          Bansos_TandaTerima.Visible := True;&#13;&#10;          Bansos_SuratPernyataanKonflik.Visible := True;&#13;&#10;          Bansos_SuratPernyataanTanggungjawab.Visible := True;              &#13;&#10;&#13;&#10;          Hibah_BAST.Visible := False;&#13;&#10;          Hibah_PaktaIntegritas.Visible := False;&#13;&#10;          Hibah_TandaTerima.Visible := False;              &#13;&#10;      end;                  &#13;&#10;end." PropData="">
