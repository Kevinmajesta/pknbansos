    <TfrxDBXQuery Name="qrySPP_1" UserName="qrySPP_1" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT&#13;&#10;      a.id_aktivitas,&#13;&#10;      a.nomor,&#13;&#10;      a.tanggal,&#13;&#10;      a.tahun,&#13;&#10;      u.nama_urusan,&#13;&#10;      sps.nomor_spd,&#13;&#10;      x.nominal nominal_spd,            &#13;&#10;      cast(substring(a.deskripsi from 1 for 5000) as varchar(5000)) deskripsi,&#13;&#10;      a.id_skpd,&#13;&#10;      s.kode_skpd_lkp,&#13;&#10;      s.nama_skpd, &#13;&#10;      vps.kode_program_skpd,&#13;&#10;      b.nama_program,  &#13;&#10;      k.kode_kegiatan_lkp,&#13;&#10;      b.nama_kegiatan,                                        &#13;&#10;      b.nominal,&#13;&#10;      b.keperluan,&#13;&#10;      b.beban,&#13;&#10;      b.jenis_beban,  &#13;&#10;      b.no_kontrak,&#13;&#10;      b.nama_penerima,&#13;&#10;      b.instansi_penerima,&#13;&#10;      b.nama_bank,&#13;&#10;      b.no_rekening_bank,&#13;&#10;      b.npwp,&#13;&#10;      b.no_dpa,&#13;&#10;      b.tanggal_dpa,&#13;&#10;      b.pagu_dpa,&#13;&#10;      b.id_bk,&#13;&#10;      bk.nama_pejabat bk_nama,&#13;&#10;      bk.nip bk_nip,&#13;&#10;      b.id_pptk,&#13;&#10;      pptk.nama_pejabat pptk_nama,&#13;&#10;      pptk.nip pptk_nip,&#13;&#10;      b.id_pa,&#13;&#10;      pa.nama_pejabat pa_nama,&#13;&#10;      pa.nip pa_nip,     &#13;&#10;      kn.nama_perusahaan,&#13;&#10;      kn.alamat_perusahaan,&#13;&#10;      kn.nama_pimpinan,&#13;&#10;      kn.tanggal_kontrak,&#13;&#10;      kn.tanggal_selesai,          &#13;&#10;      fa.lanjutan&#13;&#10;from aktivitas a&#13;&#10;join spp b on b.id_aktivitas = a.id_aktivitas&#13;&#10;join rincian_spp rs on rs.id_aktivitas=b.id_aktivitas&#13;&#10;join spp_pakai_spd sps on sps.id_aktivitas_spp = b.id_aktivitas&#13;&#10;join spd x on sps.id_aktivitas_spd = x.id_aktivitas    &#13;&#10;join v_skpd s on s.id_skpd = a.id_skpd&#13;&#10;join v_program_skpd vps on vps.id_skpd = a.id_skpd and vps.nama_program=b.nama_program and vps.id_bidang=s.id_bidang &#13;&#10;join kontrak kn on kn.no_kontrak=b.no_kontrak&#13;&#10;left join v_kegiatan_skpd k on rs.id_kegiatan = k.id_kegiatan and k.id_skpd = a.id_skpd                                                                                                                                                               &#13;&#10;join urusan u on s.id_urusan = u.id_urusan    &#13;&#10;left join pejabat_skpd bk on bk.id_pejabat_skpd = b.id_bk  &#13;&#10;left join pejabat_skpd pptk on pptk.id_pejabat_skpd = b.id_pptk     &#13;&#10;left join pejabat_skpd pa on pa.id_pejabat_skpd = b.id_pa      &#13;&#10;join v_form_anggaran_lkp fa on fa.id_skpd=a.id_skpd and fa.id_kegiatan=rs.id_kegiatan and fa.tipe='DPA221'                    &#13;&#10; where a.id_aktivitas=:id_aktivitas &#13;&#10;      " PropData="05704C65667403B4000470546F70034C010A506172616D657465727301010C29000000204E616D653D2269645F616B74697669746173222044617461547970653D226674556E6B6E6F776E220000"/>
