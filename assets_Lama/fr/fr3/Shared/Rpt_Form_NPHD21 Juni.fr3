hen result := 'Ketua'&#13;&#10;  else if (a = '2' ) then result := 'Wakil Ketua'&#13;&#10;  else if (a = '3' ) then result := 'Sekretaris'      &#13;&#10;  else result := '-';                                                                  &#13;&#10;end;&#13;&#10;  &#13;&#10;begin&#13;&#10;&#13;&#10;end.">
