    <TfrxDBXQuery Name="qryInfo" UserName="qryInfo" CloseDataSource="True" FieldAliases.Text="ID_AKTIVITAS=ID_AKTIVITAS&#13;&#10;NOMOR=NOMOR&#13;&#10;TANGGAL=TANGGAL&#13;&#10;DESKRIPSI=DESKRIPSI&#13;&#10;TAHUN=TAHUN&#13;&#10;NOMINAL=NOMINAL&#13;&#10;TANGGAL_PENGESAHAN=TANGGAL_PENGESAHAN&#13;&#10;ID_REKENING_PEKAS=ID_REKENING_PEKAS&#13;&#10;KODE_REKENING_PEKAS=KODE_REKENING_PEKAS&#13;&#10;NAMA_REKENING_PEKAS=NAMA_REKENING_PEKAS&#13;&#10;ID_SKPD=ID_SKPD&#13;&#10;KODE_SKPD_LKP=KODE_SKPD_LKP&#13;&#10;NAMA_SKPD=NAMA_SKPD&#13;&#10;USERNAME=USERNAME&#13;&#10;BEBAN=BEBAN&#13;&#10;KEPERLUAN=KEPERLUAN&#13;&#10;ID_BK=ID_BK&#13;&#10;BK_NAMA=BK_NAMA&#13;&#10;ID_PA=ID_PA&#13;&#10;PA_NAMA=PA_NAMA&#13;&#10;PA_NIP=PA_NIP&#13;&#10;ID_PPK_SKPD=ID_PPK_SKPD&#13;&#10;PPK_NAMA=PPK_NAMA&#13;&#10;PPK_NIP=PPK_NIP" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="select&#13;&#10;       a.id_aktivitas,&#13;&#10;        a.nomor,&#13;&#10;        a.tanggal,&#13;&#10;        cast(substring(a.deskripsi from 1 for 5000) as varchar(5000)) deskripsi,&#13;&#10;        a.tahun,&#13;&#10;        b.nominal,&#13;&#10;        b.tanggal_pengesahan,&#13;&#10;        b.id_rekening_pekas,&#13;&#10;        r.kode_rekening kode_rekening_pekas,&#13;&#10;        r.nama_rekening nama_rekening_pekas,&#13;&#10;        a.id_skpd,&#13;&#10;        s.kode_skpd_lkp,&#13;&#10;        s.nama_skpd,&#13;&#10;        a.username,&#13;&#10;        b.beban,&#13;&#10;        b.keperluan,&#13;&#10;        b.id_bk,&#13;&#10;        bk.nama_pejabat bk_nama,&#13;&#10;        b.id_pa,&#13;&#10;        pa.nama_pejabat pa_nama,&#13;&#10;        pa.nip pa_nip,                        &#13;&#10;        b.id_ppk_skpd,&#13;&#10;        ppk.nama_pejabat ppk_nama,  &#13;&#10;        ppk.nip ppk_nip                                         &#13;&#10;from aktivitas a&#13;&#10;join spj b on b.id_aktivitas = a.id_aktivitas&#13;&#10;join v_skpd s on a.id_skpd = s.id_skpd&#13;&#10;join rekening r on r.id_rekening = b.id_rekening_pekas&#13;&#10;left join pejabat_skpd bk on bk.id_pejabat_skpd = b.id_bk&#13;&#10;left join pejabat_skpd pa on pa.id_pejabat_skpd = b.id_pa&#13;&#10;left join pejabat_skpd ppk on ppk.id_pejabat_skpd = b.id_ppk_skpd                          &#13;&#10;where a.id_aktivitas = :id_aktivitas" PropData="05704C65667402240470546F7003D4000A506172616D657465727301010C43000000204E616D653D2269645F616B74697669746173222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B4944262336323B220000"/>
    <TfrxDBXQuery Name="qryKegiatan" UserName="qryKegiatan" CloseDataSource="True" FieldAliases.Text="ID_KEGIATAN=ID_KEGIATAN&#13;&#10;KODE_KEGIATAN_SKPD=KODE_KEGIATAN_SKPD&#13;&#10;NAMA_KEGIATAN=NAMA_KEGIATAN" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="select distinct coalesce(vk.id_kegiatan, 0) id_kegiatan, coalesce(vk.kode_kegiatan_skpd, '') kode_kegiatan_skpd, coalesce(vk.nama_kegiatan, '') nama_kegiatan&#13;&#10;from v_spj j&#13;&#10;join rincian_spj rs on rs.id_aktivitas = j.id_aktivitas&#13;&#10;left join v_kegiatan_skpd vk on vk.id_kegiatan = rs.id_kegiatan&#13;&#10;  and vk.id_skpd = j.id_skpd&#13;&#10;where j.id_aktivitas = :id_aktivitas" PropData="05704C6566740364010470546F7002240A506172616D657465727301010C43000000204E616D653D2269645F616B74697669746173222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B4944262336323B220000"/>
