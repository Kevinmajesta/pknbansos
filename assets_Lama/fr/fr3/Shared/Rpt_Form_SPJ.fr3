    <TfrxDBXQuery Name="qryRekening" UserName="qryRekening" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT&#13;&#10;        b.id_rincian_spj,&#13;&#10;        b.id_rekening,&#13;&#10;        b.id_kegiatan,&#13;&#10;        k.kode_kegiatan_skpd,&#13;&#10;        k.nama_kegiatan,&#13;&#10;        r.kode_rekening,&#13;&#10;        r.nama_rekening,&#13;&#10;        b.nominal&#13;&#10;from aktivitas a&#13;&#10;join rincian_spj b on b.id_aktivitas = a.id_aktivitas&#13;&#10;join rekening r on r.id_rekening = b.id_rekening&#13;&#10;left join v_kegiatan_skpd k on k.id_kegiatan = b.id_kegiatan  and k.id_skpd = a.id_skpd&#13;&#10;where a.id_aktivitas=:id                         " PropData="05704C65667402600470546F7003D4000A506172616D657465727301010C38000000204E616D653D226964222044617461547970653D226674537472696E67222045787072657373696F6E3D22262336303B4944262336323B220000"/>
    <TfrxDBXQuery Name="qryAnggaran" UserName="qryAnggaran" CloseDataSource="True" FieldAliases.Text="ANGGARAN=ANGGARAN" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="select coalesce(sum(ra.PAGU), 0) as anggaran                       &#13;&#10;from V_FORM_ANGGARAN_LKP fa&#13;&#10;join RINCIAN_ANGGARAN ra on ra.ID_FORM_ANGGARAN = fa.ID_FORM_ANGGARAN&#13;&#10;join TAHUN_ANGGARAN t on t.TAHUN = fa.TAHUN and fa.STATUS = t.STATUS_KINI&#13;&#10;where fa.TAHUN = :tahun and fa.ID_SKPD = :id_skpd&#13;&#10;and (fa.TIPE ='RKA21' or ('RKA21' = '-' and fa.TIPE in ('RKA21', 'RKA221', 'RKA32')))&#13;&#10;and (fa.TIPE &#60;&#62; 'RKA221' or fa.ID_KEGIATAN = :id_kegiatan)&#13;&#10;and ra.ID_REKENING = :id_rekening  " PropData=""/>
    <TfrxDBXQuery Name="qrySPJlalu" UserName="qrySPJlalu" CloseDataSource="True" FieldAliases.Text="SPJ_LALU=SPJ_LALU" BCDToCurrency="False" Master="qryAnggaran" IgnoreDupParams="False" SQL.Text="select  coalesce(sum(b.nominal), 0)  as spj_lalu&#13;&#10;from aktivitas a&#13;&#10;join rincian_spj b on b.id_aktivitas = a.id_aktivitas&#13;&#10;join rekening r on r.id_rekening = b.id_rekening&#13;&#10;left join v_kegiatan_skpd k on k.id_kegiatan = b.id_kegiatan  and k.id_skpd = a.id_skpd&#13;&#10;where r.id_rekening=:id_rekening  and a.id_aktivitas &#60; :id_aktivitas or a.tanggal &#60; :tanggal                          " PropData=""/>
