    <TfrxDBXQuery Name="qrySPJ" UserName="qrySPJ" CloseDataSource="True" FieldAliases.Text="ID_REKENING=ID_REKENING&#13;&#10;KODE_REKENING=KODE_REKENING&#13;&#10;NAMA_REKENING=NAMA_REKENING&#13;&#10;ID_SKPD=ID_SKPD&#13;&#10;ID_KEGIATAN=ID_KEGIATAN&#13;&#10;TANGGAL=TANGGAL&#13;&#10;SPJ=SPJ&#13;&#10;PAGU=PAGU&#13;&#10;SP2D=SP2D&#13;&#10;SPJ_LALU=SPJ_LALU" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="select rj.id_rekening, r.kode_rekening, r.nama_rekening, a.id_skpd, rj.id_kegiatan, a.tanggal,&#13;&#10;  coalesce(sum(rj.nominal), 0) as spj,&#13;&#10;(&#13;&#10;  select coalesce(sum(ra.volume * ra.tarif), 0)&#13;&#10;  from v_form_anggaran_lkp fa&#13;&#10;  join rincian_anggaran ra on ra.id_form_anggaran = fa.id_form_anggaran&#13;&#10;  where fa.id_skpd = a.id_skpd&#13;&#10;    and fa.tahun = :tahun&#13;&#10;    and fa.status = :status&#13;&#10;    and ra.id_rekening = rj.id_rekening&#13;&#10;    and fa.id_kegiatan = rj.id_kegiatan&#13;&#10;) pagu,&#13;&#10;(&#13;&#10;  select coalesce(sum(rs.nominal), -1)&#13;&#10;  from v_sp2d s&#13;&#10;  left join rincian_spm rs on rs.id_aktivitas = s.id_aktivitas_spm&#13;&#10;  where s.id_skpd = a.id_skpd&#13;&#10;    and s.tanggal &#60;= a.tanggal&#13;&#10;    and rs.id_rekening = rj.id_rekening&#13;&#10;    and rs.id_kegiatan = rj.id_kegiatan&#13;&#10;) sp2d,&#13;&#10;(&#13;&#10;  select coalesce(sum(ri.nominal), 0)&#13;&#10;  from v_spj jj&#13;&#10;  join rincian_spj ri on ri.id_aktivitas = jj.id_aktivitas&#13;&#10;  where jj.id_skpd = a.id_skpd&#13;&#10;    and extract(month from jj.tanggal) &#60; extract(month from a.tanggal)&#13;&#10;    and ri.id_rekening = rj.id_rekening&#13;&#10;    and ri.id_kegiatan = rj.id_kegiatan&#13;&#10;&#13;&#10;) spj_lalu&#13;&#10;from aktivitas a&#13;&#10;join rincian_spj rj on rj.id_aktivitas = a.id_aktivitas&#13;&#10;join rekening r on r.id_rekening = rj.id_rekening&#13;&#10;left outer join kegiatan k on k.id_kegiatan = rj.id_kegiatan&#13;&#10;where a.id_aktivitas = :id_aktivitas&#13;&#10;  and ((:id_kegiatan = 0 and rj.id_kegiatan is null) or rj.id_kegiatan = :id_kegiatan)&#13;&#10;group by rj.id_rekening, r.kode_rekening, r.nama_rekening, a.id_skpd, rj.id_kegiatan, a.tanggal" PropData=""/>
