    <TfrxDBXQuery Name="qryRincianSPD" UserName="qryRincianSPD" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT&#13;&#10;  RINCIAN_SPD.NOMINAL,&#13;&#10;  RINCIAN_SPD.ID_KEGIATAN,&#13;&#10;  KEGIATAN.KODE_KEGIATAN,&#13;&#10;  KEGIATAN.NAMA_KEGIATAN,&#13;&#10;  PROGRAM.KODE_PROGRAM,&#13;&#10;  PROGRAM.NAMA_PROGRAM,&#13;&#10;  a.nominal_anggaran as nominal_rka,&#13;&#10;  d.NOMINAL as NOMINAL_AKUM&#13;&#10;FROM RINCIAN_SPD&#13;&#10;  INNER JOIN KEGIATAN ON (RINCIAN_SPD.ID_KEGIATAN = KEGIATAN.ID_KEGIATAN)&#13;&#10;  INNER JOIN PROGRAM ON (KEGIATAN.ID_PROGRAM = PROGRAM.ID_PROGRAM)&#13;&#10;  Join v_form_anggaran_lkp a on a.id_kegiatan= RINCIAN_SPD.ID_KEGIATAN&#13;&#10;  JOIN SPD d on d.ID_AKTIVITAS = :id    &#13;&#10;where RINCIAN_SPD.ID_AKTIVITAS = :id&#13;&#10; AND a.id_skpd=:id_skpd              &#13;&#10;    and a.tahun = :tahun          &#13;&#10;      and a.status = :status&#13;&#10;      and a.tipe =:tipe          &#13;&#10;&#13;&#10;          " PropData=""/>
