    <TfrxDBXQuery Name="qryRinci" UserName="qryRinci" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="select &#13;&#10;p.nama_program,&#13;&#10;k.nama_kegiatan,&#13;&#10;(&#13;&#10;select coalesce(sum(fa.nominal_anggaran), 0)                                           &#13;&#10;from v_form_anggaran_lkp fa&#13;&#10;join tahun_anggaran ta on ta.tahun = fa.tahun and ta.status_kini = fa.status&#13;&#10;where fa.tahun = d.tahun&#13;&#10;  and fa.id_skpd = d.id_skpd&#13;&#10;  and ((d.beban = 'BTL' and fa.tipe = 'RKA21') or (d.beban = 'BL' and fa.tipe = 'RKA221') or (d.beban = 'KB' and fa.tipe = 'RKA32'))&#13;&#10;  and (d.beban &#60;&#62; 'BL' or (d.beban = 'BL' and fa.id_kegiatan = rd.id_kegiatan))&#13;&#10;) nominal_anggaran,&#13;&#10;(&#13;&#10;select coalesce(sum(rdd.nominal), 0)&#13;&#10;from v_spd dd&#13;&#10;join rincian_spd rdd on rdd.id_aktivitas = dd.id_aktivitas &#13;&#10;where dd.tahun = d.tahun&#13;&#10;  and dd.id_skpd = d.id_skpd&#13;&#10;  and dd.beban = d.beban&#13;&#10;  and (d.beban = 'BTL' or (d.beban = 'BL' and rdd.id_kegiatan = rd.id_kegiatan))&#13;&#10;  and dd.tanggal &#60;= d.tanggal&#13;&#10;  and dd.id_aktivitas &#60;&#62; :id    &#13;&#10;) akum_spd,&#13;&#10;rd.nominal nom_spd&#13;&#10;from v_spd d&#13;&#10;join rincian_spd rd on rd.id_aktivitas = d.id_aktivitas &#13;&#10;left join kegiatan k on k.id_kegiatan = rd.id_kegiatan &#13;&#10;left join program p on p.id_program = k.id_program&#13;&#10;where d.id_aktivitas = :id    &#13;&#10;&#13;&#10;&#13;&#10;  " PropData="05704C65667403A8000470546F7003AC000A506172616D657465727301010C1F000000204E616D653D226964222044617461547970653D226674556E6B6E6F776E2200010C1F000000204E616D653D226964222044617461547970653D226674556E6B6E6F776E220000"/>
