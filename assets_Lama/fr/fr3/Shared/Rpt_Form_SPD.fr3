d').Value            := &#60;id_skpd&#62; ;  &#13;&#10;  //qryRinci.ParamByName('tanggal').Value            := &#60;tanggal&#62; ;      &#13;&#10;    &#13;&#10;  Page1.Visible := True;      &#13;&#10;  Page2.Visible := True;&#13;&#10;  Page4.Visible := False;&#13;&#10;  Page5.Visible := False;      &#13;&#10;  qrySPD.Open;&#13;&#10;  if &#60;qrySPD.&#34;BEBAN&#34;&#62; = 'BL' then&#13;&#10;  begin&#13;&#10;    //dialogpage1.visible := true;&#13;&#10;     if (&#60;SPD&#62;='0') then&#13;&#10;  begin&#13;&#10;    Page5.Visible := True;&#13;&#10;    Page1.Visible := False;        &#13;&#10;    Page2.Visible := False;              &#13;&#10;    Page4.Visible := True;&#13;&#10;  end&#13;&#10;  else&#13;&#10;  begin&#13;&#10;    Page1.Visible := True;      &#13;&#10;    Page2.Visible := True;&#13;&#10;    Page4.Visible := False;&#13;&#10;  end;        &#13;&#10;    qryDanaSPD.SQL.Clear;        &#13;&#10;    qryDanaSPD.SQL.Add('select a.nominal_anggaran as nominal');&#13;&#10;    qryDanaSPD.SQL.Add('from form_anggaran a');&#13;&#10;    qryDanaSPD.SQL.Add('where Tahun = :tahun and Status = :status');&#13;&#10;    qryDanaSPD.SQL.Add('      and Tipe = ''RKA221'' ');&#13;&#10;    qryDanaSPD.SQL.Add('      and ID_SKPD = :id_skpd');&#13;&#10;&#13;&#10;&#13;&#10;    qryDanaSPD.Params[0].Value := &#60;qrySPD.&#34;TAHUN&#34;&#62;;&#13;&#10;    qryDanaSPD.Params[1].Value := &#60;frxDBStatus.&#34;STATUS&#34;&#62;;&#13;&#10;    qryDanaSPD.Params[2].Value := &#60;qrySKPD.&#34;ID_SKPD&#34;&#62;;&#13;&#10;&#13;&#10;    qryAkumSPD.ParamByName('tahun').Value    := &#60;frxDBStatus.&#34;TAHUN&#34;&#62;;       &#13;&#10;    qryAkumSPD.ParamByName('id_skpd').Value  := &#60;ID_SKPD&#62;;               &#13;&#10;    qryAkumSPD.ParamByName('tanggal').Value  := &#60;qrySPD.&#34;TANGGAL&#34;&#62;;&#13;&#10;    qryAkumSPD.ParamByName('beban').Value    := &#60;qrySPD.&#34;BEBAN&#34;&#62;;             &#13;&#10;    qryAkumSPD.ParamByName('id_spd').Value   := &#60;ID&#62;;&#13;&#10;&#13;&#10;  end&#13