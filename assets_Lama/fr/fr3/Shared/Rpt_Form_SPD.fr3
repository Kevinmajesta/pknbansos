    <TfrxDBXQuery Name="qryAkumSPD" UserName="qryAkumSPD" CloseDataSource="True" FieldAliases.Text="NOMINAL=NOMINAL" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="select coalesce(sum(d.NOMINAL), 0) NOMINAL                 &#13;&#10;from SPD d &#13;&#10;join AKTIVITAS a on a.ID_AKTIVITAS = d.ID_AKTIVITAS&#13;&#10;where a.TAHUN = :tahun           &#13;&#10;  and a.ID_SKPD = :id_skpd&#13;&#10;  and d.BEBAN = :beban              &#13;&#10;  and a.ID_AKTIVITAS &#60;&#62; :id_spd&#13;&#10;  and (a.TANGGAL &#60; :tanggal or (a.TANGGAL = :tanggal and a.ID_AKTIVITAS &#60; :id_spd))                                                                                                                                                                                      " PropData=""/>
    <TfrxDBXQuery Name="qryDanaSPDKeg" UserName="qryDanaSPDKeg" CloseDataSource="True" FieldAliases.Text="NOMINAL_RKA=NOMINAL_RKA" BCDToCurrency="False" Master="qryRincianSPD" IgnoreDupParams="False" SQL.Text="select coalesce(sum(a.nominal_anggaran),0) as nominal_rka&#13;&#10;from v_form_anggaran_lkp a&#13;&#10;where a.id_skpd=:id_skpd&#13;&#10;      and a.tahun = :tahun&#13;&#10;      and a.status = :status&#13;&#10;      and a.tipe ='RKA221'&#13;&#10;      and a.id_kegiatan in (select id_kegiatan from rincian_spd where id_aktivitas = :id_aktivitas)" PropData=""/>
